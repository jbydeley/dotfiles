# Display
alias ls="ls --color"
alias l="ls -lah"

# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias z="cd"

alias gd="git diff --output-indicator-new=' ' --output-indicator-old=' '"
alias gs="git status --short"

alias ga="git add --patch"
alias gc="git commit"

alias gp="git push"
alias gu="git pull"

alias gl="git log"
alias gco="git checkout"

alias gi="git init"
alias gcl="git clone"
